  "scripts": {
    "build:local": "rimraf dist && cross-env NODE_ENV=local webpack --config webpack.config.js --progress --mode development",
    "build:dev": "rimraf dist && cross-env NODE_ENV=dev webpack --config webpack.config.js --progress",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
